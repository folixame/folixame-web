<h1>New Event</h1>
<%= form_for :event, url: events_path do |f| %>    <p>
  <%= f.label :email %><br>
  <%= f.text_field :email %>
</p>

    <%= f.label :username %><br>
    <%= f.text_field :username %>
    </p>

    <%= f.label :password %><br>
    <%= f.text_field :password %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>